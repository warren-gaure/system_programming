<Window x:Class="ExternalConsole.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExternalConsole"
        xmlns:controls="clr-namespace:ExternalConsole.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="850" MinHeight="450" MinWidth="850">
    <Grid Background="#1e1e1e">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Foreground="#00A4D1" FontSize="30" TextAlignment="Center" FontWeight="Bold">🅴🅰🆂🆈🆂🅰🆅🅴 🆅3</TextBlock>
        <TextBlock Foreground="#00A4D1" FontSize="25" TextAlignment="Center" FontWeight="Bold" VerticalAlignment="Bottom">External Console</TextBlock>
        <controls:View Grid.Row="1" Grid.RowSpan="5" DataContext="{Binding }"></controls:View>
    </Grid>
</Window>
